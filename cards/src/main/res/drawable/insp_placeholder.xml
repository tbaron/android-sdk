<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape android:shape="rectangle">
			<size
				android:width="100dp"
				android:height="100dp" />
			<padding
				android:bottom="@dimen/insp_card_border_width"
				android:left="@dimen/insp_card_border_width"
				android:right="@dimen/insp_card_border_width"
				android:top="@dimen/insp_card_border_width" />
			<solid
				android:color="@color/insp_white" />
			<stroke
				android:width="@dimen/insp_card_border_width"
				android:color="@color/insp_grey_dark" />
		</shape>
	</item>
	<item>
		<bitmap
			android:gravity="center"
			android:src="@android:drawable/ic_menu_gallery" />
	</item>
</layer-list>